{
    "builds": [
      {
        "src": "massroufi_backend/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/staticfiles"
      }
    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "massroufi_backend/wsgi.py"
      }
    ]
  }
  